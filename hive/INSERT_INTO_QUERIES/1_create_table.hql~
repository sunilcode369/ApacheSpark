CREATE TABLE PERSON_NEW( ID BIGINT,
		     NAME STRING,
		     ADDRESS STRUCT<STREET1:STRING,CITY:STRING>,
		     PHONE ARRAY<STRING>,
		     BANKS_BALANCE MAP<STRING,STRING>)
ROW FORMAT DELIMITED
FIELDS TERMINATED BY ','
COLLECTION ITEMS TERMINATED BY ':'
MAP KEYS TERMINATED BY '='
LINES TERMINATED BY '\n'
STORED AS TEXTFILE;

CREATE TABLE patientdetail_TEMP(  FIRSTNAME STRING, LASTNAME STRING,
    		          ADDRESS STRUCT<HOUSENO:STRING,LOCALITYNAME:STRING,CITY:STRING>,
		           mobile STRING, LANDLINE STRING, ZIP STRING)
ROW FORMAT DELIMITED
FIELDS TERMINATED BY '|'
COLLECTION ITEMS TERMINATED BY ','
LINES TERMINATED BY '\n'
STORED AS TEXTFILE;
